{
  "name": "rosebox",
  "version": "0.2.87",
  "private": false,
  "description": "Rosebox is an experimental project with the ultimate goal to build a complete JS/TS styling framework with an emphasis on writing expressive, scalable, and conflict-free style-code.",
  "main": "es5/index.js",
  "module": "lib/index.js",
  "files": [
    "es5",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hugonteifeh/rosebox"
  },
  "_moduleAliases": {
    "@shared": "src/shared"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "test": "jest",
    "prettier:ck": "prettier --check 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "author": "Hugo Nteifeh <hugonteifeh@gmail.com>",
  "keywords": [
    "Functional css",
    "declarative styling",
    "Typed CSS"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.21",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "short-uuid": "^3.1.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.1.3",
    "@testing-library/jest-dom": "^5.3.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.7.0",
    "jest": "^24.9.0",
    "madge": "^3.9.2",
    "ts-jest": "^24.3.0",
    "typescript": "^3.9.5"
  }
}
