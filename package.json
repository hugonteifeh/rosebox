{
  "name": "rosebox",
  "version": "0.4.0",
  "private": false,
  "description": "Rosebox is a Typescript library that models CSS in Typescript in a way that leverages the capabilities of Typescript's type system and Javascript as a programming language, providing strong types, functions, and an overall more structured and improved syntax.",
  "main": "es5/index.js",
  "module": "lib/index.js",
  "files": [
    "es5",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hugonteifeh/rosebox"
  },
  "_moduleAliases": {
    "@shared": "src/shared"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "test": "jest",
    "prettier:ck": "prettier --check 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "author": "Hugo Nteifeh <hugonteifeh@gmail.com>",
  "keywords": [
    "Functional css",
    "declarative styling",
    "Typed CSS",
    "color",
    "colors"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.9.21",
    "color": "^3.1.3",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@types/ramda": "^0.27.14",
    "@types/color": "^3.0.1",
    "@testing-library/dom": "^7.1.3",
    "@testing-library/jest-dom": "^5.3.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.7.0",
    "jest": "^24.9.0",
    "madge": "^3.9.2",
    "ts-jest": "^24.3.0",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "@material-ui/styles": "^4.11.2",
    "react": "^16.13.1"
  }
}
