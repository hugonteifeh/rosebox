{
  "compilerOptions": {
    "module": "es6",
    "composite": true,
    "noEmit": false,
    "target": "ES2015",
    "moduleResolution": "node",
    "lib": ["es2019", "ESNext.Array", "dom"],
    "sourceMap": false,
    "declaration": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "jsx": "react",
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true
  },
}
